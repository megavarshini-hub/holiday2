#include <stdio.h>

int main()
{
    char ch[50];
    int i,number=0;
    printf("enter the word:");
    scanf(" %s", ch);
    for(i=0;ch[i]!='\0';i++){
    if(ch[i]>='0' && ch[i]<='9')
    {
       number = number * 10 + (ch[i] - '0'); 
       
    }
    }
    
    printf("%d", number%10);

    return 0;
}
