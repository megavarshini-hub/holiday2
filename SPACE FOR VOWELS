#include <stdio.h>

int main()
{
    char n, ch[50];
    printf("enter the word:");
    scanf(" %s", ch);
    for(int i=0; ch[i] != '\0';i++)
     if((ch[i]=='a'||ch[i]=='e'||ch[i]=='i'||ch[i]=='o'||ch[i]=='u'||ch[i]=='A'||ch[i]=='E'||ch[i]=='I'||ch[i]=='O'||ch[i]=='U'))
     {
         printf(" ");
     }else {
            printf("%c", ch[i]);
        }
    return 0;
}
